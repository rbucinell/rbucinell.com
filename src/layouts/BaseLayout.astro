---
import HeadComponent from "../components/HeadComponent.astro";
import WrapperComponent from "../components/WrapperComponent.astro";
import "../styles/global.css";

const { config } = Astro.props;
let title = config.title || 'Ryan Bucinell\'s Personal Site';
---
<html lang="en" class="bg-gray-200">
  <HeadComponent title={title}></HeadComponent>
  <body>
    <WrapperComponent config={config}>
      <slot />
    </WrapperComponent>
  </body>
</html>
