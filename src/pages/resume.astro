---
import BaseLayout from '../layouts/BaseLayout.astro';
import TechIconComponent from '../components/TechIconComponent.astro';
import ExperienceComponent from '../components/ExperienceComponent.astro';

const config = {
    title: "Ryan Bucinell - Résumé",
    breadcrumbs: [
        {fa:"home", text:"Home", url:"index.html"},
        {fa:"info",text:"Personal Info"},
        {fa:"file-text",text:"Resume", url:"#"}
    ],
    page: {
        title: 'Ryan Bucinell',
        small: 'A Curriculum Vitae'
    }
};
const fdir = "img/websites/";
---
<BaseLayout config={config}>

    <h4 class="center"><a href="data/resume/Resume%20for%20Ryan%20Michael%20Bucinell.pdf">Download A PDF Version</a></h4>
    <h2 class="collapse-icon"> <a data-toggle="collapse" data-target="#experienceSection" aria-expanded="true">Experience</a> </h2>

    <div id="experienceSection" class="collapse show border-left">
        <ExperienceComponent title="Senior Software Engineer" company="Draft Kings" daterange="September 2022 - Current" img="img/resume/draftkings.png"
            descriptions={['TBD']}/>
        
        <ExperienceComponent title="Software Developer" company="Day Automation" daterange="November 2021 - August 2022" img="img/resume/day.png"
            descriptions={['Full-stack development of internal site with ruby-on-rails backend and pure JavaScript front-end utilizing REST calls.',
            'Maintained and developed AutoCAD extension, implementing authentication and PDF generation',
            'Automated ticket tracking system integrating Redmine and Trello services.']} />
        
        <ExperienceComponent title="Software Engineer" company="Paychex" daterange="November 2016 - June 2019 - November 2021" img="img/resume/paychex.png"
            descriptions={['[Nov-2016] Contractor; [Jun-2019] Unified Communications; [Oct-2020] Unified Service Development',
            'Designed and developed a ExpressJS/Mongo DB REST service to execute GUI automation of system changes ',
            'De-compile, analyze, document, and re-engineer projects with missing GIT source control',
            'Redesigned ASP.NET / AngularJS website for network and Unified Communications automation and monitoring',
            'Automation scripts to support Cisco VXML Java applications & BMC Orchestrator in ServiceNow ticket manager',
            'Trained and on-boarded multiple new engineers o existing project/technologies and agile development practices.']} />

        <ExperienceComponent title="Software Engineer" company="Harris via Mindex Technologies" daterange="May 2014 - November 2016" img="img/resume/harris.png"
            descriptions={[
            'Application development for Windows and mobile platforms',
            'Rewritten legacy applications to support new technologies while maintaining and expanding functionality',
            'Utilized Technologies: Version control systems, visual studio, SQL, C#, XML, & XSLT']} />
        <ExperienceComponent title="Software Engineer" company="Xerox Corporation via Mindex Technologies" daterange="January 2013 - May 2014" img="img/resume/xerox.png"
            descriptions={[
            'Worked on scrum team development utilizing TFS for team code management',
            'Update and maintain large-scale enterprise software for multiple projects',
            'Created backend services to migrate data, and generate reports as well as manage other services']} />
        <ExperienceComponent title="Performance Engineer" company="Xerox Corporation via Mindex Technologies" daterange="November 2011 - January 2013" img="img/resume/xerox.png"
            descriptions={[
            'Design, build, and deployed international multi-stage metrics gathering system with automated reporting',
            'Retrofit and optimize tools to generate large amounts of big data utilized in server stress simulation',
            'Analyze, Debug, and Report code performance, utilizing PAL, ANTS Profiler, and other performance tools']} />
    </div>
    

    <h2 class="collapse-icon"><a data-toggle="collapse" data-target="#langSection" aria-expanded="true">Languages</a></h2>
    <style>
        #langSection {
            display:grid;
            grid-template-columns: repeat( 4, 1fr);
        }
    </style>
    <div class="collapse show border-left container" id="langSection">
        {['python', 'csharp', 'cplusplus', 'java', 'html5', 'css3', 'javascript', 'typesecript']
            .map( i => <TechIconComponent icon={i} word="false" colored="true"/>) }
    </div>


    <h2 class="collapse-icon">
        <a data-toggle="collapse" data-target="#techSection" aria-expanded="true">Technologies</a>
    </h2>
    <style>
        #techSection {
            display:grid; 
            grid-template-columns: repeat(5,1fr);
        }
    </style>
    <div id="techSection" class="collapse show border-left container">
        {['dot-net', 'angularjs', 'bootstrap', 'visualstudio', 'oracle','git','nodejs', 'gulp','intellij', 'mongodb', 'npm']
            .map( i => <TechIconComponent icon={i} word="false" colored="true"/>) }
    </div>
</BaseLayout>

